{
  "name": "webpack-demo",
  "version": "0.0.13",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=5.6.0"
  },
  "scripts": {
    "test": "eslint --format=node_modules/eslint-formatter-pretty ./{src,test}/**/*.{js,jsx} && nyc ava",
    "testen": "testen -- ava",
    "build": "webpack",
    "watch": "webpack --watch",
    "update": "updtr && ava-codemods --force"
  },
  "testen": {
    "node": [
      "4.3.1",
      "5.6.0",
      "5.7.0"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "xo-react",
      "plugin:xo/recommended",
      "plugin:ava/recommended",
      "plugin:react/recommended"
    ],
    "plugins": [
      "xo",
      "ava",
      "react"
    ]
  },
  "babel": {
    "env": {
      "node4": {
        "presets": [
          "es2015-node4"
        ]
      },
      "node5": {
        "presets": [
          "es2015-node5"
        ]
      },
      "node6": {
        "presets": [
          "es2015-node6"
        ]
      },
      "development": {
        "presets": [
          "es2015"
        ]
      }
    },
    "presets": [
      "react"
    ]
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.14.0",
    "ava-codemods": "^0.2.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-node4": "^2.1.0",
    "babel-preset-es2015-node5": "^1.2.0",
    "babel-preset-es2015-node6": "^0.1.3",
    "babel-preset-react": "^6.5.0-1",
    "babel-register": "^6.7.2",
    "coveralls": "^2.11.9",
    "css-loader": "^0.23.1",
    "eslint": "^2.8.0",
    "eslint-config-xo": "^0.13.0",
    "eslint-config-xo-react": "^0.7.0",
    "eslint-formatter-pretty": "^0.2.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-ava": "^2.2.1",
    "eslint-plugin-react": "^5.0.1",
    "eslint-plugin-xo": "^0.3.1",
    "file-loader": "^0.8.5",
    "nyc": "^6.4.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "style-loader": "^0.13.1",
    "testen": "^1.8.1",
    "updtr": "^0.1.10",
    "webpack": "^2.1.0-beta.6"
  }
}
